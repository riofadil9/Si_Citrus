<body>
    <div class="flex flex-col justify-center items-center content-center">
        <img src="/uploads/<%= item.gambar %>" alt="">
        <p>Username: <%= item.username %></p>
        <p>Hasil: <%= JSON.stringify(item.hasil.class) %></p>
        <p>Akurasi: <%= JSON.stringify(item.hasil.score) %></p>
        <p>Waktu: <%= new Date(item.waktu).toLocaleString() %></p>
    </div>
</body>